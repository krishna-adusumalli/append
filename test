FROM ubuntu:latest

RUN apt-get update \
    && apt-get install -y apache2 \
    && apt-get clean \
    && rm -rf /var/lib/apt/lists/*

RUN rm -rf /var/www/html/*

RUN apt-get update && apt-get install -y git
COPY /home/ubuntu/Append/ /var/www/html/
RUN apt update 
EXPOSE 80

CMD service apache2 start && apachectl -D FOREGROUND
#test
